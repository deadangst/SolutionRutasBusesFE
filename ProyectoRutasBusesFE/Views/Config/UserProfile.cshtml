@model dynamic

@{
    ViewData["Title"] = "Perfil del Usuario";
}

<h2>Perfil del Usuario</h2>

<div class="container">
    <div class="row">
        <div class="col-md-6">
            <h3>Detalles del Perfil</h3>
            <p><strong>Nombre:</strong> @Model.Nombre</p>
            <p><strong>Email:</strong> @Model.Email</p>
            <p><strong>Tipo de Usuario:</strong> @Model.TipoUsuario</p>
        </div>
    </div>
</div>
